
@{
    ViewBag.Title = "YearlyBookings";
}

<h2>Yearly Bookings Report</h2>

<table class="table">
    <tr>
        <th>
            Year
        </th>
       
        <th>
            Total Bookings
        </th>
        <th></th>
    </tr>

    @foreach (var item in ViewBag.res)
    {
        <tr>
            <td>
                @item.year
            </td>
           
            <td>
                @item.Sum
            </td>

        </tr>
    }

</table>